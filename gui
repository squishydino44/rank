local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("SkidWare", "DarkTheme")
local Tab = Window:NewTab("Hood Modded")
local Section = Tab:NewSection("Aimlock")
Section:NewButton("Aimlock", "(q) custom ciaz aimlock", function()
  getgenv().RecurringPoint = "UpperTorso"
  getgenv().Hitbox = "UpperTorso"
  getgenv().Keybind = "q"
  getgenv().AimbotStrengthAmount = 1
  getgenv().PredictionAmount = 8
  getgenv().Radius = 32
  getgenv().UsePrediction = true
  getgenv().AimbotStrength = true
  getgenv().FirstPerson = true
  getgenv().ThirdPerson = false
  getgenv().TeamCheck = false
  getgenv().Enabled = true
  getgenv().AutoPrediction = true
	loadstring(game:HttpGet("https://raw.githubusercontent.com/squishydino44/rank/main/aim"))()
	
  loadstring(game:HttpGet("https://raw.githubusercontent.com/squishydino44/rank/main/aim%20auto%20predict"))()
  
  game.StarterGui:SetCore("SendNotification",  {
		Title = "SkidWare";
		Text = "AIMBOT ON";
		Icon = "8569049326";
		Duration = 1;
		})
end)
Section:NewButton("ESP", "(v) simple esp", function()
     loadstring(game:HttpGet('https://raw.githubusercontent.com/squishydino44/rank/main/toggle%20esp'))()
     
     game.StarterGui:SetCore("SendNotification",  {
		Title = "SkidWare";
		Text = "ESP ON";
		Icon = "7018414259";
		Duration = 1;
		})
end)
Section:NewButton("Silent Aim", "(x) use against losers", function()
  _G.Prediction = .18
  _G.FOV = 666
  _G.AimKey = "x"
	loadstring(game:HttpGet('https://raw.githubusercontent.com/squishydino44/rank/main/dhm%20silent%20aim'))()
	
	game.StarterGui:SetCore("SendNotification",  {
		Title = "SkidWare";
		Text = "SILENT AIM ON";
		Icon = "7018414259";
		Duration = 1;
		})
end)
Section:NewButton("Dot Silent Aim", "(z) silent aim with dot", function()
	_G.KEY = "z" -- Key to lock player
	_G.PART = "UpperTorso" -- Part to lock victim's bodypart
	_G.PRED = 0.160 -- Prediction
	_G.Frame = Vector3.new(0, 0.53, 0) -- Idk what this is dont touch it tho
	loadstring(game:HttpGet('https://raw.githubusercontent.com/squishydino44/rank/main/dhm%20dot%20silent'))()
	
	game.StarterGui:SetCore("SendNotification",  {
		Title = "SkidWare";
		Text = "DIT SILENT AIM ON";
		Icon = "7018414259";
		Duration = 1;
		})
end)
Section:NewButton("Fly", "(c) Fly and shoot", function()
	loadstring(game:HttpGet('https://raw.githubusercontent.com/squishydino44/rank/main/cframe%20fly'))()
	
	game.StarterGui:SetCore("SendNotification",  {
		Title = "SkidWare";
		Text = "FLY ON";
		Icon = "7018414259";
		Duration = 1;
		})
end)
Section:NewButton("Chat Spy", "read chat logs", function()
	loadstring(game:HttpGet('https://raw.githubusercontent.com/squishydino44/rank/main/chat%20spy'))()
	
	game.StarterGui:SetCore("SendNotification",  {
		Title = "SkidWare";
		Text = "CHAT SPY ON";
		Icon = "7018414259";
		Duration = 1;
		})
end)
Section:NewKeybind("Toggle UI", "do not use zxcv", Enum.KeyCode.B, function()
	Library:ToggleUI()
end)
